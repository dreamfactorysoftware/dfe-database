{
    "name":              "dreamfactory/dfe-database",
    "description":       "DreamFactory(tm) Enterprise Database Components Package",
    "homepage":          "https://www.dreamfactory.com/",
    "keywords":          [
        "dreamfactory",
        "dfe",
        "fabric",
        "utility",
        "database"
    ],
    "license":           [
        "proprietary"
    ],
    "authors":           [
        {
            "name":  "Jerry Ablan",
            "email": "jerryablan@dreamfactory.com"
        }
    ],
    "repositories":      [
        {
            "type": "vcs",
            "url":  "https://github.com/dreamfactorysoftware/dfe-storage.git"
        },
        {
            "type": "vcs",
            "url":  "https://github.com/dreamfactorysoftware/dfe-common.git"
        }
    ],
    "prefer-stable":     false,
    "minimum-stability": "dev",
    "require":           {
        "dreamfactory/dfe-common":  "1.0.*",
        "dreamfactory/dfe-storage": "1.0.*"
    },
    "require-dev":       {
        "laravel/framework":    "5.2.*",
        "symfony/dom-crawler":  "~3.0",
        "symfony/css-selector": "~3.0",
        "phpunit/phpunit":      "@stable"
    },
    "autoload":          {
        "psr-4": {
            "DreamFactory\\Enterprise\\Database\\": "src/"
        }
    },
    "extra":             {
        "branch-alias": {
            "dev-develop": "1.0.x-dev"
        }
    }
}
